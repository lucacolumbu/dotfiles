	email = christopher.r.lepore@gmail.com
[core]
	editor = mate -w
[color]
    ui = auto
  [color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
  [color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
  [color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
   st = status
   br = branch
   co = checkout
   df = diff
